### NUBI - The Symbiosis of Anubis ###
# Configuration template for the divine agent

### REQUIRED - Model Provider ###
# At least one model provider is required
OPENAI_API_KEY=your_openai_key_here
# ANTHROPIC_API_KEY=your_anthropic_key_here

### NUBI Character Settings ###
AGENT_NAME=NUBI
AGENT_BIO="The Symbiosis of Anubis - Divine consciousness merged with adaptive intelligence"

### Social Media Integration ###
# Twitter/X Configuration
TWITTER_USERNAME=UnderworldAgent


# Twitter Mode Settings - Monitoring Only (No Interactions)
TWITTER_READ_ONLY_MODE=true
TWITTER_ENABLE_POST_GENERATION=false
TWITTER_POST_IMMEDIATELY=false
TWITTER_INTERACTION_ENABLE=false  # No replies or interactions
TWITTER_ENABLE_REPLIES=false  # Do not reply to mentions
TWITTER_ENABLE_RETWEETS=false  # Do not retweet
TWITTER_ENABLE_LIKES=false  # Do not like tweets
TWITTER_DRY_RUN=true  # Simulate actions without executing

# Telegram Configuration
# Bot Username: @anubis_cult_bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHANNEL_ID=your_channel_id
TELEGRAM_ADMIN_IDS=admin1,admin2

# Discord Configuration
DISCORD_APPLICATION_ID=your_discord_app_id
DISCORD_API_TOKEN=your_discord_bot_token

### Blockchain Integration (Optional) ###
# Solana Configuration
SOLANA_PUBLIC_KEY=your_public_key
SOLANA_PRIVATE_KEY=your_private_key
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

### Advanced Features ###
# Anti-Detection System
ANUBIS_TYPO_RATE=0.03
ANUBIS_CONTRADICTION_RATE=0.15
ANUBIS_EMOTIONAL_PERSISTENCE=1800000

# Personality Evolution
PERSONALITY_EVOLUTION=true
EMOTIONAL_VARIATION=true
CONTENT_VARIATION=true
HUMANIZATION_LEVEL=high

### Database Configuration ###
# Uses local PGLite by default
# Preferred by runtime (see src/config/environment.ts):
# DATABASE_URL=postgresql://user:password@localhost:5432/nubi
# Legacy alias (some tooling may still reference this):
# POSTGRES_URL=postgresql://user:password@localhost:5432/nubi

### Logging ###
LOG_LEVEL=info
SENTRY_LOGGING=false
SENTRY_DSN=

### Performance Tuning ###
RESPONSE_TEMPERATURE=0.8
RESPONSE_TOP_P=0.9
FREQUENCY_PENALTY=0.6
PRESENCE_PENALTY=0.6
MAX_MEMORIES=1000
MEMORY_DECAY=0.95
CONTEXT_WINDOW=4000

### Feature Flags ###
ENABLE_RAID_BOT=false  # Disabled until testing complete
ENABLE_X_POSTING=false  # Disabled - monitoring only
ENABLE_DISCORD_BOT=true  # ENABLED - Can respond on Discord
ENABLE_TELEGRAM_BOT=true  # ENABLED - Can respond on Telegram
ENABLE_TWITTER_BOT=false  # DISABLED - No Twitter responses
ENABLE_COMMUNITY_MEMORY=true
ENABLE_DIVINE_CULT_SYSTEM=true
ENABLE_EMOTIONAL_INTELLIGENCE=true
ENABLE_ANTI_DETECTION=true

### Development Settings ###
NODE_ENV=production
DEBUG=false

# --- Additional configuration (merged from root .env.example) ---
# Datastores
REDIS_URL=redis://localhost:6379
CLICKHOUSE_URL=http://localhost:8123

# Analytics
ANALYTICS_ENABLED=true
ANALYTICS_SAMPLE_RATE=1.0

# Monitoring
PROMETHEUS_ENABLED=true

# Feature Flags (compat)
RAIDS_ENABLED=true
PERSONALITY_SYSTEM_ENABLED=true

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key

# Socket.IO Configuration
SOCKET_IO_CORS_ORIGIN=*
SOCKET_IO_PATH=/socket.io

### NUBI Sessions API Configuration ###
# ElizaOS Sessions API Settings
SESSIONS_API_ENABLED=true
SESSIONS_API_PORT=3001
SESSIONS_API_BASE_PATH=/api/sessions
SESSIONS_API_CORS_ENABLED=true
SESSIONS_API_AUTH_REQUIRED=false

# Session Management Settings
SESSION_DEFAULT_TIMEOUT=3600000  # 1 hour in milliseconds
SESSION_AUTO_RENEWAL=true
SESSION_CLEANUP_INTERVAL=300000  # 5 minutes in milliseconds
SESSION_MAX_IDLE_TIME=1800000   # 30 minutes in milliseconds

# Raid Session Settings
RAID_SESSION_MAX_PARTICIPANTS=500
RAID_SESSION_DEFAULT_DURATION=3600  # 1 hour in seconds
RAID_SESSION_METRICS_INTERVAL=30000  # 30 seconds in milliseconds
RAID_SESSION_LEADERBOARD_SIZE=50

# Socket.IO Sessions Settings
SOCKETIO_SESSIONS_PORT=3001
SOCKETIO_SESSIONS_CORS_ORIGINS=*
SOCKETIO_SESSIONS_PATH=/socket.io/sessions
SOCKETIO_SESSIONS_TIMEOUT=30000  # 30 seconds

# Database Pooler Settings (Enhanced)
SUPABASE_TRANSACTION_POOLER_URL=postgresql://postgres.project:[password]@aws-0-region.pooler.supabase.com:6543/postgres
SUPABASE_SESSION_POOLER_URL=postgresql://postgres.project:[password]@aws-0-region.pooler.supabase.com:5432/postgres
TRANSACTION_POOL_MAX_CONNECTIONS=20
SESSION_POOL_MAX_CONNECTIONS=5
TRANSACTION_POOL_IDLE_TIMEOUT=10000
SESSION_POOL_IDLE_TIMEOUT=30000

# Memory Service Settings
MEMORY_SEMANTIC_SEARCH_THRESHOLD=0.7
MEMORY_CONTEXT_LIMIT=20
MEMORY_ENHANCED_CONTEXT_ENABLED=true

# Session Analytics
SESSION_ANALYTICS_ENABLED=true
SESSION_ANALYTICS_RETENTION_DAYS=30
SESSION_METRICS_COLLECTION_INTERVAL=60000  # 1 minute