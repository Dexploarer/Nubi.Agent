    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "test:actions": "bun test src/__tests__/actions.test.ts",
    "test:providers": "bun test src/__tests__/provider.test.ts",
    "test:services": "bun test src/__tests__/services.test.ts",
    "test:integration": "bun test src/__tests__/integration.test.ts",
    "test:unit": "bun test src/__tests__/*.test.ts",
    "test:all": "bun test src/**/*.test.ts",
    "setup:mcp": "bun scripts/setup-mcp.ts",
    "mcp:test": "npx -y @promptordie/xmcpx@latest --help"
